{
    "name": "Wind - IDF Speed Control",
    "version": "0.0.1",
    "manifest_version": 2,
    "author": "thelittlewonder",
    "description": "Change the playback speed of IDF Videos!",
    "homepage_url": "https://www.github.com/littlewonder/wind",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "content_scripts": [
        {
            "matches": [
        "https://www.interaction-design.org/courses/*"
      ],
            "css": ["css/inject.css"],
            "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
